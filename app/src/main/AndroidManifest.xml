<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.fstracker.foodstoragetracker">

    <application
        android:name="final_project.fstracker.foodstoragetracker.FoodStorageApplication"
        android:allowBackup="true"
        android:icon="@drawable/logo"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name="final_project.fstracker.foodstoragetracker.ModifyItemActivity"
            android:label="@string/activity_modifyItem_title"
            android:parentActivityName="final_project.fstracker.foodstoragetracker.MenuActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="final_project.fstracker.foodstoragetracker.MenuActivity" />
        </activity>
        <activity
            android:name="final_project.fstracker.foodstoragetracker.SettingsActivity"
            android:label="@string/activity_settings_title"
            android:parentActivityName="final_project.fstracker.foodstoragetracker.MenuActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="final_project.fstracker.foodstoragetracker.MenuActivity" />
        </activity>
        <activity
            android:name="final_project.fstracker.foodstoragetracker.ViewItemActivity"
            android:label="@string/activity_ViewItem_title"
            android:parentActivityName="final_project.fstracker.foodstoragetracker.MenuActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="final_project.fstracker.foodstoragetracker.MenuActivity" />
        </activity>
        <activity
            android:name="final_project.fstracker.foodstoragetracker.ViewListActivity"
            android:label="@string/activity_ViewList_title"
            android:parentActivityName="final_project.fstracker.foodstoragetracker.MenuActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="final_project.fstracker.foodstoragetracker.MenuActivity" />
        </activity>
        <activity
            android:name="final_project.fstracker.foodstoragetracker.ViewListByNameActivity"
            android:label="@string/activity_ViewList_title"
            android:parentActivityName="final_project.fstracker.foodstoragetracker.MenuActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="final_project.fstracker.foodstoragetracker.MenuActivity" />
        </activity>
        <activity
            android:name="final_project.fstracker.foodstoragetracker.DeleteItemActivity"
            android:label="@string/activity_deleteItem_title" />
        <activity
            android:name="final_project.fstracker.foodstoragetracker.AddItemActivity"
            android:label="@string/activity_addItem_title"
            android:parentActivityName="final_project.fstracker.foodstoragetracker.MenuActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="final_project.fstracker.foodstoragetracker.MenuActivity" />
        </activity>
        <activity android:name="final_project.fstracker.foodstoragetracker.MenuActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>